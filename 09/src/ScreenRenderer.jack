
class ScreenRenderer {

    function void print(String str) {
        do Output.printString(str);
        do str.dispose();
        return;
    }

    function int locationToMemoryAdress32(int x, int y) {
        let x = x * 2;
        let y = y * 32 * 2* 16;
        return 16384 + x + y;
    }

