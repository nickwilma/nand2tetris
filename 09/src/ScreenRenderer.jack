    function void drawTileBorder(int size_x, int size_y, int size_tile) {
        var int x, y;
        let x = 0;
        do Screen.setColor(true);
        while (~(x = size_x)) {
            let y = 0;
            while (~(y = size_y)) {
                // top
                do Screen.drawLine((x * size_tile), (y * size_tile), (x * size_tile) + (size_tile - 1), (y * size_tile));
                // right
                do Screen.drawLine((x * size_tile) + (size_tile - 1), (y * size_tile), (x * size_tile) + (size_tile - 1), (y * size_tile) + (size_tile - 1));
                // bottom
                do Screen.drawLine((x * size_tile), (y * size_tile) + (size_tile - 1), (x * size_tile) + (size_tile - 1), (y * size_tile) + (size_tile - 1));
                // left
                do Screen.drawLine((x * size_tile), (y * size_tile), (x * size_tile), (y * size_tile) + (size_tile - 1));

                let y = y + 1;
            }
            //do Output.println();
            let x = x + 1;
        }
        return;
    }

    function void drawBorderTilesCrosses32(int size_x, int size_y) {
        var int x, y;
        let x = 0;
        do Screen.setColor(true);
        while (~(x = size_x)) {
            let y = 0;
            while (~(y = size_y)) {
                // top left
                do Screen.drawLine((x * 32), (y * 32), (x * 32) + 5, (y * 32));
                // top right
                do Screen.drawLine((x * 32) + (31 - 5), (y * 32), (x * 32) + (32 - 1), (y * 32));
                // right top
                do Screen.drawLine((x * 32) + (32 - 1), (y * 32), (x * 32) + (32 - 1), (y * 32) + 5);
                // right bottom
                do Screen.drawLine((x * 32) + (32 - 1), (y * 32) + (31 - 5), (x * 32) + (32 - 1), (y * 32) + (32 - 1));
                // bottom - right
                do Screen.drawLine((x * 32) + (31 - 5), (y * 32) + (32 - 1), (x * 32) + (32 - 1), (y * 32) + (32 - 1));
                // bottom - left
                do Screen.drawLine((x * 32), (y * 32) + (32 - 1), (x * 32) + 5, (y * 32) + (32 - 1));
                // left - bottom
                do Screen.drawLine((x * 32), (y * 32) + (31 - 5), (x * 32), (y * 32) + (32 - 1));
                // left - top
                do Screen.drawLine((x * 32), (y * 32), (x * 32), (y * 32) + 5);

                let y = y + 1;
            }
            //do Output.println();
            let x = x + 1;
        }
        return;
    }

